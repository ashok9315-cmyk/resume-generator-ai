{
  "name": "ats-resume-builder",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:smoke": "npm run test:integration -- --testPathPattern=smoke",
    "logs:tail": "aws logs tail /aws/lambda/ats-resume-generateResume --follow",
    "logs:errors": "aws logs tail /aws/lambda/ats-resume-generateResume --filter-pattern '{ $.level = \"error\" }' --follow",
    "deploy:full": "node scripts/deployment/deploy-full.js",
    "deploy:lambda": "node scripts/deployment/deploy-lambda-only.js",
    "deploy:frontend": "node scripts/deployment/deploy-frontend-only.js",
    "test:deployment": "node scripts/testing/test-deployment.js",
    "deploy:staging": "cd infrastructure && cdk deploy --all --context env=staging",
    "deploy:prod": "cd infrastructure && cdk deploy --all --context env=production",
    "dashboard:open": "aws cloudwatch get-dashboard --dashboard-name ats-resume-dashboard",
    "traces:view": "aws xray get-trace-summaries --start-time $(date -u -d '1 hour ago' +%s) --end-time $(date -u +%s)"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@aws-sdk/client-cloudwatch": "^3.958.0",
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/s3-request-presigner": "^3.654.0",
    "@langchain/anthropic": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "aws-xray-sdk-core": "^3.7.0",
    "axios": "^1.13.2",
    "date-fns": "^3.6.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "langchain": "^0.3.0",
    "langsmith": "^0.2.0",
    "mammoth": "^1.7.0",
    "next": "14.2.0",
    "pdf-parse": "^1.1.1",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-dropzone": "^14.2.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.14.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0"
  }
}
